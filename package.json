{
  "description": "Blockly based kano coding editor",
  "name": "@kano/kwc-blockly",
  "version": "3.1.17-beta.3",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0",
    "@webcomponents/webcomponentsjs": "2.0.0-beta.2"
  },
  "scripts": {
    "serve": "es6-server",
    "serve-test": "web-tester serve 'test/**/*.test.js'",
    "test": "web-tester run ./test/*.test.js",
    "test-ci": "yarn run --silent test -r xunit > test-results.xml",
    "coverage": "web-tester cover ./test/*.test.js",
    "coverage-ci": "yarn run --silent coverage -r cobertura -r markdown",
    "lint": "eslint ./*.js ./lib ./field ./input",
    "lint-ci": "yarn lint -f checkstyle -o eslint.xml"
  },
  "dependencies": {
    "@kano/kwc-color-picker": "^3.0.0-beta.8",
    "@kano/kwc-number-inputs": "^0.0.11",
    "@kano/kwc-style": "^3.0.0-beta.2",
    "@polymer/iron-a11y-keys": "^3.0.0-pre.18",
    "@polymer/iron-flex-layout": "^3.0.0-pre.18",
    "@polymer/iron-resizable-behavior": "^3.0.1",
    "@polymer/paper-dialog": "^3.0.0-pre.18",
    "@polymer/paper-styles": "^3.0.0-pre.18",
    "@polymer/polymer": "^3.0.0"
  },
  "devDependencies": {
    "@kano/es6-server": "^1.0.3",
    "@kano/eslint-config": "^1.0.0",
    "@kano/web-tester": "^1.0.0-alpha.20",
    "@polymer/iron-test-helpers": "^3.0.1",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.16.0",
    "replace": "^1.0.1"
  }
}
